#include <stdio.h>
#include <math.h>

int main() {
    int num, originalNum, remainder, result = 0, n = 0;

  
    scanf("%d", &num);

    originalNum = num;


    while (originalNum != 0) {
        originalNum /= 10;
        n++;
    }

    originalNum = num;

  
    while (originalNum != 0) {
        remainder = originalNum % 10;
        result += pow(remainder, n);
        originalNum /= 10;
    }


    if (result == num)
        printf("Yes\n");
    else
        printf("No\n");

    return 0;
}
