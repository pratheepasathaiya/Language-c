#include <stdio.h>

int isElementPresent(int arr[], int size, int key) {
    for(int i = 0; i < size; i++) {
        if(arr[i] == key) {
            return 1; // Found
        }
    }
    return 0; // Not found
}

int main() {
    int n, searchElement;

    
    scanf("%d", &n);

    int arr[n];

    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    scanf("%d", &searchElement);

    if(isElementPresent(arr, n, searchElement)) {
        printf("%d is presented in an array.\n", searchElement);
    } else {
        printf("%d is not presented in an array.\n", searchElement);
    }

    return 0;
}
